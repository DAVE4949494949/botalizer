<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for site in sites %}
    <url>
        <loc>http://botalizer.com/website/{{ site.site }}</loc>
        <lastmod>{{ (site.update_date - 24*60*60)|date('Y-m-d') }}</lastmod>
    </url>
    {% endfor %}
</urlset>